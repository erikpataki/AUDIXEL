{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "build"],
    "includePattern": ".+\\.(js|jsx)$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}